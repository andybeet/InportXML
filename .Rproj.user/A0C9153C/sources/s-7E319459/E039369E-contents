d <- read.csv("InportXML-template.csv")
dmast <- read.csv("Master_DataSet_Template.csv")

library(dplyr)

#d$ind <- seq(1,nrow(d),1)
dmast$ind <- seq(1,nrow(dmast),1)


test <- left_join(dmast, d, by = c("tag","level"))

out <- test %>% filter(!duplicated(ind)) %>% select(-c(instructions.x, value.y))


names(out)


write.csv(out, "Master_DataSet_Template2.csv")
      
    
      